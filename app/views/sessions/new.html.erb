<body>
    <div class="container">
        <div class="signinform row" id="signinform-top">
            <!-- Beginrow -->
            <div class="signincol col-sm-6">
                <!-- Begin signin form -->
                <ul class="list-group list-group-flush">
                  <li class="list-group-item">
                    <h1 class="signin">Sign in
                    </hi>
                  </li>

                    <%= form_tag sessions_path do %>
                    <li class="list-group-item">
                        <%= label_tag :email %>
                        <br>
                        <%= text_field_tag :email %>
                    </li>
                    <li class="list-group-item">
                        <%= label_tag :password %>
                        <br>
                        <%= password_field_tag :password %>
                    </li>
                    <li class="list-group-item actions">
                        <%= submit_tag("Log In", :class => "login-submit-button" ) %>
                    </li>
                    <% end %>
                </ul>


                <!-- <div class='col-md-6' style='text-align:center !important; margin-top:1em'>
    								<%= link_to :controller => "sessions", :action => "new", :kind => "Brand", class: "homebtns" do %>
    									<div id="brand-button" class='col-md-11 center-block new-user-button' style='padding: 10px' nowrap>Brand
    									</div>
    								<% end %>
    						</div> -->




            </div>
            <!-- End form -->
            <div class="signincol col-sm-6">
                <!-- signin info -->
                <%= image_tag "login-collage.jpg", id: "login-top-image" %>
            </div>
            <!-- end info-->
        </div>
        <!-- end row -->
    </div>
    <!--end container
</body>
